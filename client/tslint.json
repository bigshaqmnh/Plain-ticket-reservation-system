{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ordered-imports": false,
    "trailing-comma": false,
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "no-console": {
      "severity": "warning"
    },
    "no-shadowed-variable": {
      "severity": "warning"
    }
  },
  "linterOptions": {
    "exclude": [
      "node_modules"
    ]
  },
  "rulesDirectory": []
}
